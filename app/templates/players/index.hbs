<h1>Players</h1>
<div class="row">
  <div class="well well-lg">
    Testing basic hasMany relationships
  </div>
</div>
<div class="row">
  {{#each}}
    <div class='col-lg-6' style="min-height:300px">
      <h2>{{name}} - {{squad.name}}</h2>
      <h4>Weapons</h4>
      <ol>
        {{#each weapons}}
          <li>{{name}}</li>
        {{/each}}
      </ol>
      <h4>Medals</h4>
      <ol>
        {{#each medals}}
          <li>{{name}}</li>
        {{/each}}
      </ol>
    </div>
  {{/each}}
</div>